#include <iostream>
using namespace std;


template <typename T>
void Swap(T& a,T& b)
{
	T tmp;
	tmp = a;
	a = b;
	b = tmp;
}

template <typename T>
class op{
public:
	T pp(T v)
	{
		return v*v;
	}
};

int main()
{
	int a = 2;
	int b = 3;

	Swap(a,b);

	cout << "a = "<<a <<"   b="<<b <<endl;

	op<int> test;

	cout << "calc="<< test.pp(10)<< endl;
	
	op<float> test1;

	cout << "calc="<< test1.pp(0.1)<< endl;

	return 0;
}
